        <Label x:Name="label" Content="Menu:" HorizontalAlignment="Left" Margin="40,24,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Sair" HorizontalAlignment="Left" Margin="178,242,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
